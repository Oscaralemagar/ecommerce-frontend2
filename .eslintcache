(\n        <>\n        \n        <div className=\"col-md-7\">\n           {images && images.length ? <Carousel showArrows={true} autoPlay infiniteLoop>\n                {images && images.map((i)=><img src={i.url} key={i.public_id}/>)}\n            </Carousel> : \n            <Card\n            cover={\n                <img \n                src={Laptop} \n                    style={{height: \"200px\", objectFit: 'cover'}}\n                    className=\"mb-3 card-image\"\n                    />}\n            >\n\n            </Card>\n            }\n            <Tabs type=\"card\">\n                <TabPane tab=\"Description\" key=\"1\">\n                    {description && description}\n                </TabPane>\n                <TabPane tab=\"Comment\" key=\"2\">\n                   Call us On XXXXXXX to know more about this product\n                </TabPane>\n            </Tabs>\n        </div>\n\n        <div className=\"col-md-5\">\n        <h1 className=\"bg-info p-3\">{title}</h1>\n        {product && product.ratings && product.ratings.length>0 ?\n        showAverage(product): \n        (<div className=\"text-center pt-1 pb-3\">No rating Yet</div>)}\n  \n        <Card\n          actions={[\n            \n              <Tooltip title={tooltip}>\n                <a onClick={handleAddToCart}>\n                 <ShoppingCartOutlined \n                 className=\"text-danger\"/><br />\n                 Add to Cart </a>\n                </Tooltip>,\n            <>\n            <a onClick={handleAddToWishlist}>\n                 <HeartOutlined className=\"text-info\" /><br />\n                 Add to Wishlist</a> </>,\n                       <RatingModal>\n                       <StarRating \n                       name={_id}\n                       numberOfStars={5}\n                       rating={star}\n                       changeRating={onStarClick}\n                     isSelectable={true}\n                       starRatedColor=\"red\"\n                       />\n                       </RatingModal>,\n        ]}\n        >\n          <ProductListItems product={product} />\n           \n          \n        \n            </Card>\n        </div>\n        </>\n    )\n\n}\nexport default SingleProduct","/Users/omprakashmagar/Desktop/ecommerce/client/src/components/cards/ProductListItems.js",[],"/Users/omprakashmagar/Desktop/ecommerce/client/src/components/modal/RatingModal.js",["386"],"import React, { useState } from 'react'\nimport {Modal, Button} from 'antd'\nimport {toast} from 'react-toastify'\nimport {useSelector} from 'react-redux'\nimport {StarOutlined} from '@ant-design/icons'\nimport {useHistory, useParams} from 'react-router-dom'\nconst RatingModal=({children})=>{\n    const {user}=useSelector((state)=>({...state}));\n    const [modalVisible, setModalVisible]=useState(false);\n\n    let history=useHistory();\n    let {slug}=useParams()\n   \n    const handleModal=()=>{\n        if(user && user.token){\n            setModalVisible(true)\n        } else {\n            history.push({\n                pathname: '/login',\n                state: { from : `/product/${slug}` }\n            })\n        }\n    }\n\n    return (\n        <>\n        <div onClick={handleModal}>\n            <StarOutlined className=\"text-danger\"/><br/>\n            {user ? \"Leave rating\" : 'Log in to leave rating'}\n        </div>\n        <Modal\n        title=\"Leeave your rating\"\n        centered\n        visible={modalVisible}\n        onOk={()=>{\n            setModalVisible(false)\n            toast.success('Thanks for your review. It will apear soon');\n        }}\n        onCancel={()=>setModalVisible(false)}\n        >{children}</Modal>\n        </>\n\n    )\n}\nexport default RatingModal","/Users/omprakashmagar/Desktop/ecommerce/client/src/functions/rating.js",[],"/Users/omprakashmagar/Desktop/ecommerce/client/src/components/category/CategoryList.js",[],"/Users/omprakashmagar/Desktop/ecommerce/client/src/pages/category/Categ